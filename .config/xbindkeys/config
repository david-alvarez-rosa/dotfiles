"pamixer -i 5"
  XF86AudioRaiseVolume
"pamixer -i 10"
  Shift + XF86AudioRaiseVolume
"pamixer -i 1"
  Control + XF86AudioRaiseVolume

"pamixer -d 5"
  XF86AudioLowerVolume
"pamixer -d 10"
  Shift + XF86AudioLowerVolume
"pamixer -d 1"
  Control + XF86AudioLowerVolume

"pamixer -t"
  XF86AudioMute

"xbacklight -inc 5"
  XF86MonBrightnessUp
"xbacklight -inc 10"
  Shift + XF86MonBrightnessUp
"xbacklight -inc 1"
  Control + XF86MonBrightnessUp

"xbacklight -dec 5"
  XF86MonBrightnessDown
"xbacklight -dec 10"
  Shift + XF86MonBrightnessDown
"xbacklight -dec 1"
  Control + XF86MonBrightnessDown

"mpc toggle"
  XF86AudioPlay
"mpc prev"
  XF86AudioPrev
"mpc next"
  XF86AudioNext

"if xinput list-props 'ELAN1200:00 04F3:309F Touchpad' | grep 'Device Enabled (149):.*1'; then xinput disable 'ELAN1200:00 04F3:309F Touchpad'; else xinput enable 'ELAN1200:00 04F3:309F Touchpad'; fi"
  XF86TouchpadToggle

"slock"
  Mod2 + Mod4 + l

"maim | tee ~/tmp/$(date +'%Y%m%d-%H:%M:%S').png | xclip -selection clipboard -t image/png"
  Print
"maim --window $(xdotool getactivewindow) | tee ~/tmp/$(date +'%Y%m%d-%H:%M:%S').png | xclip -selection clipboard -t image/png"
  Shift + Print
"maim --select | tee ~/tmp/$(date +'%Y%m%d-%H:%M:%S').png | xclip -selection clipboard -t image/png"
  Control + Print

"dmenu_run"
  Mod4 + d
